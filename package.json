{
  "name": "electron-with-vite-remix",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "out/main/index.mjs",
  "scripts": {
    "dev:renderer": "pnpm exec electron dev-server.mjs",
    "dev:main": "cross-env NODE_ENV=development pnpm dev:main_",
    "dev:main_": "pnpm --filter main exec vite build --mode development && pnpm --filter preload exec vite build --mode development && electron out/main/index.mjs",
    "build": "turbo run build && electron-builder",
    "build:unpack": "turbo run build && electron-builder --dir",
    "typecheck": "turbo run typecheck"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "electron": "^36.9.5",
    "electron-builder": "^26.8.1",
    "turbo": "^2"
  },
  "packageManager": "pnpm@9.14.4",
  "exports": {
    ".": {
      "import": "./out/main/index.mjs"
    }
  },
  "author": "tmtk75"
}
